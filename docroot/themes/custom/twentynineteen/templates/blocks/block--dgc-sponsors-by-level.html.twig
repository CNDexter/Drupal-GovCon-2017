{% if content.sponsor_listing['#view'].result|length > 0 %}
  {% set classes = [
  'sponsor-listing',
  'sponsor-listing--' ~ content.sponsor_level,
  ]%}
  <div {{ attributes.addClass(classes) }}>
    <h2>Thanks to our {{ content.sponsor_level }} Sponsors</h2>
    {{ content.sponsor_listing }}
  </div>
{% endif %}
