{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
    'footer',
  ]
%}
{% if content %}
  <div{{ attributes.addClass(classes) }}>
    {{ content }}
    <div class="footer__links">
      <div class="footer__links-icons">
        <a href="https://www.facebook.com/DrupalGovCon">
            {{source('/themes/custom/twentyeighteen/images/icon-facebook.svg')}}
        </a>
        <a href="https://www.twitter.com/DrupalGovCon">
          {{source('/themes/custom/twentyeighteen/images/icon-twitter.svg')}}
        </a>
        <a href="mailto:drupal4gov@gmail.com?subject=Contact from Website">
          {{source('/themes/custom/twentyeighteen/images/icon-email.svg')}}
        </a>
      </div>
    </div>
    <div class="footer__logo">
      {{source('/themes/custom/twentyeighteen/images/GC18_logo_solid_rev.svg')}}
    </div>
  </div>
{% endif %}
